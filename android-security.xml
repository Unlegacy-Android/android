<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- patched AOSP repos from android-security remote, no device specific changes needed -->

  <project path="external/libavc" name="android_external_libavc" remote="android-security" groups="pdk" />
  <remove-project name="platform/external/libhevc" />
  <project path="external/libhevc" name="android_external_libhevc" remote="android-security" groups="pdk-fs" />
  <remove-project name="platform/external/libmpeg2" />
  <project path="external/libmpeg2" name="android_external_libmpeg2" remote="android-security" groups="pdk" />
  <remove-project name="platform/external/sfntly" />
  <project path="external/sfntly" name="android_external_sfntly"  remote="android-security" groups="pdk,qcom_msm8x26" />

  <remove-project name="platform/platform/frameworks/opt/net/wifi" />
  <project path="frameworks/opt/net/wifi" name="android_frameworks_opt_net_wifi" remote="android-security"groups="pdk" />

  <remove-project name="platform/hardware/qcom/audio" />
  <project path="hardware/qcom/audio" name="android_hardware_qcom_audio" remote="android-security" groups="qcom,qcom_audio" />

  <remove-project name="platform/packages/apps/CertInstaller" />
  <project path="packages/apps/CertInstaller" name="android_packages_apps_CertInstaller" remote="android-security" />

</manifest>
