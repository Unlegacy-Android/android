<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- patched AOSP repos from android-security remote, no device specific changes needed -->
  <remove-project name="platform/external/libavc" />
  <remove-project name="platform/external/libhevc" />
  <remove-project name="platform/external/libvpx" />
  <remove-project name="platform/external/skia" />
  <remove-project name="platform/external/sonivox" />
  <remove-project name="platform/external/sqlite" />
  <remove-project name="platform/external/tremolo" />
  <remove-project name="platform/frameworks/minikin" />
  <remove-project name="platform/packages/apps/Messaging" />
  <remove-project name="platform/packages/apps/Nfc" />
  <remove-project name="platform/packages/apps/Settings" />
  <remove-project name="platform/system/bt" />

  <project path="external/libavc" name="android_external_libavc" remote="android-security" groups="pdk" />
  <project path="external/libhevc" name="android_external_libhevc" remote="android-security" groups="pdk-fs" />
  <project path="external/libvpx" name="android_external_libvpx" remote="android-security" groups="pdk" />
  <project path="external/skia" name="android_external_skia" remote="android-security" groups="pdk" />
  <project path="external/sonivox" name="android_external_sonivox" remote="android-security" groups="pdk" />
  <project path="external/sqlite" name="android_external_sqlite" remote="android-security" groups="pdk" />
  <project path="external/tremolo" name="android_external_tremolo" remote="android-security" groups="pdk" />
  <project path="frameworks/minikin" name="android_frameworks_minikin" remote="android-security" groups="pdk-cw-fs,pdk-fs" />
  <project path="packages/apps/Messaging" name="android_packages_apps_Messaging" remote="android-security" />
  <project path="packages/apps/Nfc" name="android_packages_apps_Nfc" remote="android-security" />
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="android-security" />
  <project path="system/bt" name="android_system_bt" remote="android-security" groups="pdk" />

</manifest>
