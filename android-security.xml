<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Remove AOSP Repos we needed to patch -->
  <remove-project name="device/common" />
  <remove-project name="platform/external/aac" />
  <remove-project name="platform/external/chromium-webview" />
  <remove-project name="platform/external/doclava" />
  <remove-project name="platform/external/guava" />
  <remove-project name="platform/external/libavc" />
  <remove-project name="platform/external/libhevc" />
  <remove-project name="platform/external/libmpeg2" />
  <remove-project name="platform/external/libvorbis" />
  <remove-project name="platform/external/libvpx" />
  <remove-project name="platform/external/wpa_supplicant_8" />
  <remove-project name="platform/frameworks/ex" />
  <remove-project name="platform/frameworks/minikin" />
  <remove-project name="platform/packages/apps/Email" />
  <remove-project name="platform/packages/apps/PackageInstaller" />
  <remove-project name="platform/packages/apps/Settings" />
  <remove-project name="platform/packages/apps/UnifiedEmail" />
  <remove-project name="platform/packages/providers/MediaProvider" />
  <remove-project name="platform/packages/providers/UserDictionaryProvider" />
  <remove-project name="platform/packages/services/Telephony" />
  <remove-project name="platform/system/bt" />
  <remove-project name="platform/system/media" />

  <!-- Patched AOSP Repos -->
  <project path="device/common" name="android_device_common" remote="android-security" />
  <project path="external/aac" name="android_external_aac" remote="android-security" groups="pdk" />
  <project path="external/chromium-webview" name="android_external_chromium-webview" remote="android-security" groups="pdk" />
  <project path="external/libavc" name="android_external_libavc" remote="android-security" />
  <project path="external/libhevc" name="android_external_libhevc" remote="android-security" />
  <project path="external/libmpeg2" name="android_external_libmpeg2" remote="android-security" />
  <project path="external/libvpx" name="android_external_libvpx" remote="android-security" groups="pdk" />
  <project path="external/wpa_supplicant_8" name="android_external_wpa_supplicant_8" remote="android-security" groups="pdk" />
  <project path="frameworks/ex" name="android_frameworks_ex" remote="android-security" groups="pdk-cw-fs,pdk-fs" />
  <project path="frameworks/minikin" name="android_frameworks_minikin" remote="android-security" groups="pdk-cw-fs,pdk-fs" />
  <project path="packages/apps/Email" name="android_packages_apps_Email" remote="android-security" groups="pdk-fs" />
  <project path="packages/apps/PackageInstaller" name="android_packages_apps_PackageInstaller" remote="android-security" />
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="android-security" />
  <project path="packages/apps/UnifiedEmail" name="android_packages_apps_UnifiedEmail" remote="android-security" groups="pdk-fs" />
  <project path="packages/providers/MediaProvider" name="android_packages_providers_MediaProvider" groups="pdk-cw-fs,pdk-fs" />
  <project path="packages/providers/UserDictionaryProvider" name="android_packages_providers_UserDictionaryProvider" groups="pdk-cw-fs,pdk-fs" />
  <project path="packages/services/Telephony" name="android_packages_services_Telephony" remote="android-security" groups="pdk-cw-fs,pdk-fs" />
  <project path="system/bt" name="android_system_bt" remote="android-security" />
  <project path="system/media" name="android_system_media" remote="android-security" groups="pdk" />

  <!-- For JDK 8-->
  <project path="external/doclava" name="android_external_doclava" remote="android-security" groups="pdk-cw-fs,pdk-fs" />
  <project path="external/guava" name="android_external_guava" remote="android-security" groups="pdk-cw-fs,pdk-fs" />

</manifest>
