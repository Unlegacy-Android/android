<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- SamkenX Android manifest project -->
  <project path="android" name="SamkenX-Android/android" remote="samkenxstrean" />

  <!-- SamkenX Android vendor project  -->
  <project path="vendor/samkenx" name="SamkenX-Android/android_vendor_samkenx" remote="samkenxstream" />

  <!-- Replace build project to build kernel inline from the source -->
  <remove-project name="platform/build" />
  <project path="build" name="SamkenX-Android/android_build" remote="samkenxstream" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <remove-project name="platform/frameworks/av" />
  <project path="frameworks/av" name="SamkenX-Android/android_frameworks_av" remote="samkenxstream" groups="pdk" />
  <remove-project name="platform/frameworks/base" />
  <project path="frameworks/base" name="SamkenX-Android/android_frameworks_base" remote="samkenxstream" groups="pdk" />
  <remove-project name="platform/frameworks/native" />
  <project path="frameworks/native" name="SamkenX-Android/android_frameworks_native" remote="samkenxstream" groups="pdk" />
  <remove-project name="platform/system/bt" />
  <project path="system/bt" name="SamkenX-Android/android_system_bt" remote="samkenxstream" groups="pdk" />
  <remove-project name="platform/packages/apps/Camera2" />
  <project path="packages/apps/Camera2" name="SamkenX-Android/android_packages_apps_Camera2" remote="samkenxstream" groups="pdk-fs" />
  <remove-project name="platform/system/core" />
  <project path="system/core" name="SamkenX-Android/android_system_core" remote="samkenxstream" groups="pdk" />

  <project path="external/stlport" name="SamkenX-Android/android_external_stlport" remote="samkenxstream" groups="pdk" />

  <project path="prebuilts/gcc/linux-x86/arm/arm-eabi-4.7" name="platform/prebuilts/gcc/linux-x86/arm/arm-eabi-4.7" revision="master" groups="pdk,linux,arm" />

  <!-- debloat -->
  <remove-project name="device/asus/deb" />
  <remove-project name="device/asus/flo" />
  <remove-project name="device/asus/flo-kernel" />
  <remove-project name="device/asus/fugu" />
